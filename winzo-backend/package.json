{
  "name": "winzo-backend",
  "version": "2.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "node railway-start-simple.js",
    "start:prod": "NODE_ENV=production node railway-start-simple.js",
    "dev": "nodemon src/server.js",
    "migrate": "node src/database/migrate.js",
    "migrate:prod": "NODE_ENV=production node src/database/migrate.js",
    "railway:start": "node railway-start-simple.js",
    "railway:simple": "node railway-start-simple.js",
    "railway:full": "node railway-start.js",
    "test": "NODE_ENV=production node tests/api-integration.test.js",
    "test:integration": "NODE_ENV=production node tests/api-integration.test.js",
    "test:ci": "NODE_ENV=production node tests/api-integration.test.js",
    "test:debug": "NODE_ENV=production node tests/direct-registration-test.js",
    "test:local": "TEST_API_URL=http://localhost:5000 node tests/api-integration.test.js",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "prestart": "npm run lint",
    "postinstall": "echo 'Backend dependencies installed successfully!'",
    "healthcheck": "node -e \"console.log('Backend is healthy')\""
  },
  "keywords": [
    "winzo",
    "sports-betting",
    "gaming",
    "big-win-energy"
  ],
  "author": "WINZO Team",
  "license": "ISC",
  "description": "WINZO Backend API - Sports Betting Platform with Big Win Energy",
  "dependencies": {
    "axios": "^1.6.0",
    "node-cache": "^5.1.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "compression": "^1.7.4",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1"
  }
}

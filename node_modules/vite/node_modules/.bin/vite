#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Natha/OneDrive/Documents/_Projects/winzo/Site/node_modules/.pnpm/vite@7.2.2_@types+node@24.1_8299f192230dd2c4027f7f11cedda3ed/node_modules/vite/bin/node_modules:/mnt/c/Users/Natha/OneDrive/Documents/_Projects/winzo/Site/node_modules/.pnpm/vite@7.2.2_@types+node@24.1_8299f192230dd2c4027f7f11cedda3ed/node_modules/vite/node_modules:/mnt/c/Users/Natha/OneDrive/Documents/_Projects/winzo/Site/node_modules/.pnpm/vite@7.2.2_@types+node@24.1_8299f192230dd2c4027f7f11cedda3ed/node_modules:/mnt/c/Users/Natha/OneDrive/Documents/_Projects/winzo/Site/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Natha/OneDrive/Documents/_Projects/winzo/Site/node_modules/.pnpm/vite@7.2.2_@types+node@24.1_8299f192230dd2c4027f7f11cedda3ed/node_modules/vite/bin/node_modules:/mnt/c/Users/Natha/OneDrive/Documents/_Projects/winzo/Site/node_modules/.pnpm/vite@7.2.2_@types+node@24.1_8299f192230dd2c4027f7f11cedda3ed/node_modules/vite/node_modules:/mnt/c/Users/Natha/OneDrive/Documents/_Projects/winzo/Site/node_modules/.pnpm/vite@7.2.2_@types+node@24.1_8299f192230dd2c4027f7f11cedda3ed/node_modules:/mnt/c/Users/Natha/OneDrive/Documents/_Projects/winzo/Site/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi

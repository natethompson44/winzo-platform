# WINZO Frontend - Production Environment Variables
# Copy this file to .env.production and fill in your values

# API Configuration
REACT_APP_API_URL=https://winzo-platform-production.up.railway.app

# Environment
REACT_APP_ENVIRONMENT=production

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# Debugging (should be false in production)
REACT_APP_DEBUG_MODE=false

# Build Configuration
NODE_ENV=production
GENERATE_SOURCEMAP=false

# Optional: Custom branding
REACT_APP_BRAND_NAME=WINZO
REACT_APP_SUPPORT_EMAIL=support@winzo.com 